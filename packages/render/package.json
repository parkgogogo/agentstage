{
  "name": "@agentstage/render",
  "version": "0.2.2",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -f tsconfig.tsbuildinfo && tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./bridge-state-provider": {
      "types": "./dist/bridge-state-provider.d.ts",
      "default": "./dist/bridge-state-provider.js"
    },
    "./shadcn": {
      "types": "./dist/shadcn.d.ts",
      "default": "./dist/shadcn.js"
    },
    "./json-to-tsx": {
      "types": "./dist/json-to-tsx.d.ts",
      "default": "./dist/json-to-tsx.js"
    }
  },
  "peerDependencies": {
    "@json-render/core": "*",
    "@json-render/react": "*",
    "@json-render/shadcn": "*",
    "react": ">=18",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@json-render/core": "0.7.0",
    "@json-render/react": "0.7.0",
    "@json-render/shadcn": "^0.7.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
